{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cuben/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cuben/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/cuben",
  "projects": {
    "cuben": {
      "type": "application",
      "root": "apps/cuben",
      "entryFile": "main",
      "sourceRoot": "apps/cuben/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cuben/tsconfig.app.json"
      }
    },
    "cuben-batch": {
      "type": "application",
      "root": "apps/cuebn-batch",
      "entryFile": "main",
      "sourceRoot": "apps/cuben-batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cuebn-batch/tsconfig.app.json"
      }
    }
  }
}